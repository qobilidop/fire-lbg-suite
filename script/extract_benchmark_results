#!/usr/bin/env python
from pathlib import Path


with Path('results.csv').open('w') as f:
    f.write('run, time_in_sec\n')
    for path in sorted(Path('.').glob('*/output')):
        ctimes = [p.stat().st_ctime for p in path.glob('*')]
        dt = max(ctimes) - min(ctimes)
        f.write(f'{path.parent.name}, {dt}\n')
